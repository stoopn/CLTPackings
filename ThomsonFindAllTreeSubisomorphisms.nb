       RowBox[{"[", 
        RowBox[{
         RowBox[{"Values", "[", "map", "]"}], ",", 
         StyleBox[
          RowBox[{"Values", "[", "map", "]"}],
          FontWeight->"Bold"]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
