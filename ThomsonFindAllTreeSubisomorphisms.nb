   3.702987519567142*^9, 3.702987519659871*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"AllCoords", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "AllSubisos", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AllAdjs", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "AllSubisos", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"m", "=", "1"}], ",", 
    RowBox[{"m", "\[LessEqual]", 
     RowBox[{"Length", "[", "AllSubisos", "]"}]}], ",", " ", 
    RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"map", "=", 
      RowBox[{"AllSubisos", "[", 
       RowBox[{"[", "m", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"inv", "=", 
      RowBox[{"Apply", "[", 
       RowBox[{"Normal", ",", 
        RowBox[{"GroupBy", "[", 
