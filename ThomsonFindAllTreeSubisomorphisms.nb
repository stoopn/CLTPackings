Cell[BoxData[
 RowBox[{
  RowBox[{"AllSubisos", " ", "=", " ", 
   RowBox[{"IGVF2FindSubisomorphisms", "[", 
    RowBox[{"GRC", ",", "T1G"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7029108944992867`*^9, 3.702910902385735*^9}, {
