Cell["Now repeat everything for the T01 Thomson graph:", "Text",
 CellChangeTimes->{{3.70298733218887*^9, 3.7029873468366337`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"TADJ", "=", 
   RowBox[{
    RowBox[{"Import", "[", "\"\<ADJ_Thomson2.mat\>\"", "]"}], "//", 
    "First"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"TV", " ", "=", " ", 
   RowBox[{"Import", "[", "\"\<Thomson2.mat\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"T1Coords", "=", 
   RowBox[{"TV", "[", 
    RowBox[{"[", "3", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"T1G", "=", 
   RowBox[{"AdjacencyGraph", "[", 
    RowBox[{"Round", "[", "TADJ", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"a1", "=", 
   RowBox[{"GraphAutomorphismGroup", "[", "T1G", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"NAutThomson", "=", 
   RowBox[{"GroupOrder", "[", "a1", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.702985951613701*^9, 3.70298598745216*^9}, {
  3.7029874512360287`*^9, 3.7029874674891644`*^9}}],

