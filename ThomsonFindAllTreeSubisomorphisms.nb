       RowBox[{"Don", "'"}], "t", " ", "need", " ", "to", " ", "project", " ",
        "on", " ", "unit", " ", "sphere", " ", "because", " ", "already", " ",
        "so", " ", "by", " ", "construction"}], " ", "*)"}], 
